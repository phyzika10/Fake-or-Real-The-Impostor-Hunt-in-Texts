   "source": [
    "# # Fake News Detection: Final Optimized Ensemble\n",
    "# **With Proper Variable Scoping**\n",
    "\n",
    "# ======================\n",
    "# 1. INITIALIZATION & DATA LOADING\n",
    "# ======================\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# Initialize with your latest LB score\n",
    "lb_score = 0.912  # Replace with your actual score\n",
    "\n",
    "# Load model predictions\n",
    "dfs = []\n",
    "for path in [\n",
    "    \"/kaggle/input/submission/submission Combinig feature.csv\",\n",
    "    \"/kaggle/input/submission/submission Enssenbel.csv\",\n",
    "    \"/kaggle/input/submission/submission Random Forest.csv\", \n",
    "    \"/kaggle/input/submission/submission memory Optimized.csv\",\n",
    "    \"/kaggle/input/submission/submission_challenger_deberta_only Memory Optimized.csv\"\n",
    "]:\n",
    "    df = pd.read_csv(path)\n",
    "    if 'real_text_id' in df.columns:\n",
    "        df['label'] = df['real_text_id'] - 1  # Convert to 0/1\n",
    "    dfs.append(df)\n",
    "\n",
    "# ======================\n",
    "# 2. CORE ENSEMBLE FUNCTIONS (WITH PROPER SCOPING)\n",
    "# ======================\n",
    "def generate_optimized_submission(dfs, lb_score):\n",
    "    # Calculate weighted sum\n",
    "    weights = [0.265, 0.215, 0.15, 0.1, 0.08]\n",
    "    weighted_sum = sum(df['label']*w for df,w in zip(dfs, weights)) / sum(weights)\n",
    "    \n",
    "    # Identify controversial samples\n",
    "    all_preds = pd.concat([df['label'] for df in dfs], axis=1)\n",
    "    controversy = all_preds.std(axis=1)\n",
    "    controversial = controversy > 0.37\n",
    "    n_controversial = controversial.sum()\n",
    "    \n",
    "    # Adjust controversial samples\n",
    "    adjusted_sum = weighted_sum.copy()\n",
    "    adjusted_sum[controversial] = (\n",
    "        0.55*dfs[0]['label'][controversial] + \n",
    "        0.3*dfs[1]['label'][controversial] + \n",
    "        0.15*weighted_sum[controversial]\n",
    "    )\n",
    "    \n",
    "    # LB-aware class balancing\n",
    "    target_ratio = 0.535 + (lb_score - 0.91) * 0.3\n",
    "    target_ratio = np.clip(target_ratio, 0.52, 0.55)\n",
    "    threshold = 0.5 + ((adjusted_sum > 0.5).mean() - target_ratio)/4\n",
    "    \n",
    "    final_preds = (adjusted_sum > threshold).astype(int)\n",
    "    \n",
    "    return final_preds, n_controversial, target_ratio\n",
    "\n",
    "# ======================\n",
    "# 3. GENERATE & ANALYZE SUBMISSION\n",
    "# ======================\n",
    "# Generate predictions\n",
    "final_preds, n_controversial, target_ratio = generate_optimized_submission(dfs, lb_score)\n",
    "\n",
    "# Create submission\n",
    "submission = pd.DataFrame({\n",
    "    'id': dfs[0]['id'],\n",
    "    'real_text_id': final_preds + 1  # Convert back to 1/2\n",
    "})\n",
    "\n",
    "# Analysis\n",
    "class_dist = submission['real_text_id'].value_counts(normalize=True)\n",
    "\n",
    "# Visualization\n",
    "plt.figure(figsize=(10,4))\n",
    "sns.histplot(\n",
    "    pd.concat([df['label'] for df in dfs], axis=1).std(axis=1), \n",
    "    bins=30, \n",
    "    kde=True\n",
    ")\n",
    "plt.axvline(0.37, color='red', linestyle='--', label='Controversy Threshold')\n",
    "plt.title(f\"Model Disagreement ({n_controversial} controversial samples)\")\n",
    "plt.xlabel(\"Prediction Standard Deviation\")\n",
    "plt.legend()\n",
    "plt.show()\n",
    "\n",
    "# Final output\n",
    "print(\"\\n🎯 Final Class Distribution:\")\n",
    "print(class_dist.round(3))\n",
    "print(\"\\n✅ Submission saved with:\")\n",
    "print(f\"- Real News Target: {target_ratio:.1%} | Achieved: {class_dist[1]:.1%}\")\n",
    "print(f\"- Controversial Samples Adjusted: {n_controversial}\")\n",
    "print(f\"- LB-Score Used: {lb_score:.3f}\")\n",
    "\n",
    "# Save\n",
    "submission.to_csv(\"final_optimized_submission.csv\", index=False)"
   ]
